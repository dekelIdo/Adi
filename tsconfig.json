{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "ES2022",
    "useDefineForClassFields": false,
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "importHelpers": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
